{
    "name": "plients/netcup",
    "description": "A Netcup Client for PHP",
    "keywords": [
        "api",
        "netcup",
        "soap",
        "vserver"
    ],
    "license": "MPL-2.0",
    "authors": [
        {
            "name": "Brian Faust",
            "email": "hello@basecode.sh",
            "homepage": "https://basecode.sh"
        }
    ],
    "require": {
        "php": "^7.1"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^8.2"
    },
    "autoload": {
        "psr-4": {
            "Plients\\Netcup\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Plients\\Tests\\Netcup\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
